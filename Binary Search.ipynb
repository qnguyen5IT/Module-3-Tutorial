   "source": [
    "def binary_search(arr, k): \n",
    "    low = 0\n",
    "    high = len(arr) - 1\n",
    "\n",
    "    while low <= high: \n",
    "        mid = (low + high) // 2\n",
    "        if arr[mid] == k: \n",
    "            return mid\n",
    "        elif arr[mid] < k: \n",
    "            low = mid + 1\n",
    "        else: \n",
    "            high = mid - 1\n",
    "    return -1"
   ]
  }
